{
  "name": "js-tdd-course",
  "version": "1.0.0",
  "description": "Biblioteca do curso de JS TDD",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm test",
    "lint": "./node_modules/.bin/eslint src/*.js"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": ["text", "html"],
    "exclude": ["tests"]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:coverage"
    }
  },
  "keywords": [
    "js",
    "tdd"
  ],
  "author": "Gide√£o Silva <gideaoms@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gideaoms/js-tdd-course.git"
  },
  "bugs": {
    "url": "https://github.com/gideaoms/js-tdd-course/issues"
  },
  "homepage": "https://github.com/gideaoms/js-tdd-course#readme",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^1.0.0-rc.1",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1"
  }
}
